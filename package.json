{
  "name": "todo-chat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watchify": "watchify -v -w -t [ babelify --presets [ react ] ] bin/*.js -o public/javascripts/app.js",
    "jshint": "jshint models/*.js app.js routes/*.js",
    "build": "webpack",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "jade": "~1.11.0",
    "jsx-transform": "^2.3.0",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "pg": "~6.1.2",
    "postgresql": "0.0.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "sequelize": "~3.30.1",
    "sequelize-cli": "~2.5.1",
    "serve-favicon": "~2.3.2",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
    "jshint": "^2.9.4",
    "supervisor": "~0.12.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
